<Application x:Class="SeaData.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="AppWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/VS2012WindowStyle.xaml" />
                <ResourceDictionary Source="Themes/DarkOrange/Core.xaml"/>
                <ResourceDictionary Source="Themes/DarkOrange/Icons.xaml"/>
                <ResourceDictionary Source="Themes/DarkOrange/Brushes.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
